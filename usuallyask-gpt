proxy-groups:
  - name: "dns-out"
    type: dns

  - name: "入口选择"
    type: select
    use:
      - entrance

  - name: "出口选择"
    type: select
    use:
      - exit

  - name: "Chain-代理链"
    type: relay
    proxies:
      - "入口选择"
      - "出口选择"

  - name: "🚀 节点选择"
    type: select
    proxies:
      - DIRECT
      - Chain-代理链
      - 入口选择

  - name: "🎬 流媒体解锁"
    type: select
    proxies:
      - Chain-代理链
      - 入口选择
      - DIRECT

  - name: "🌍 国际流量"
    type: select
    proxies:
      - Chain-代理链
      - 入口选择
      - DIRECT

  - name: "📺 YouTube/Netflix/Disney+"
    type: select
    proxies:
      - 🎬 流媒体解锁
      - Chain-代理链
      - 入口选择
